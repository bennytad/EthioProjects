@model IEnumerable<HRS.Models.Rooms>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.room_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RoomTypes.room_type_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.floor_level)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.note)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.room_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RoomTypes.room_type_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.floor_level)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.note)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.room_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.room_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.room_id })
        </td>
    </tr>
}

</table>
